{
  "name": "skoleskyss-dashboard",
  "description": "Dashboard for skoleskyss",
  "version": "1.1.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "engines": {
    "node": ">=8.5.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build && next export",
    "start": "next start",
    "now-deploy": "npm run build && now --static out -n dashboard-skoleskyss && now alias"
  },
  "keywords": [
    "dashboard",
    "next",
    "skoleskyss",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/skoleskyss-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/skoleskyss-dashboard/issues"
  },
  "homepage": "https://github.com/telemark/skoleskyss-dashboard",
  "now": {
    "name": "dashboard-skoleskyss",
    "alias": [
      "dashboard.skoleskyss.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "coveralls": "2.13.1",
    "nsp": "2.8.0",
    "nyc": "11.2.1",
    "standard": "10.0.3"
  },
  "dependencies": {
    "axios": "0.16.2",
    "muicss": "0.9.26",
    "next": "3.2.2",
    "react": "15.6.2",
    "react-dom": "15.6.2"
  }
}
